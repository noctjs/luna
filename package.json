{
  "name": "@noctjs/luna",
  "version": "0.1.0",
  "main": "dist/luna.cjs.js",
  "module": "dist/luna.esm.js",
  "types": "dist/luna.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rm -rf build dist",
    "build": "yarn run build:compile && yarn run build:typings",
    "build:compile": "rollup -c",
    "build:typings": "tsc --declaration --emitDeclarationOnly --outDir build && api-extractor run && rm -rf build"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.7.0",
    "@rollup/plugin-replace": "^2.2.1",
    "rollup": "^1.27.10",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-typescript2": "^0.25.3",
    "typescript": "^3.7.3"
  },
  "license": "MIT",
  "keywords": [
    "ecs",
    "game"
  ]
}
